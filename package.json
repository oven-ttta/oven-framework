{
  "name": "oven",
  "version": "0.1.1",
  "description": "A full-stack web framework powered by Bun - Next.js style routing with blazing fast performance",
  "type": "module",
  "main": "./src/index.ts",
  "bin": {
    "oven": "./bin/oven.js"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "bin",
    "README.md"
  ],
  "scripts": {
    "dev": "bun run bin/oven.js dev",
    "build": "bun run bin/oven.js build",
    "start": "bun run bin/oven.js start",
    "build:vercel": "bun run scripts/build-static.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "bun",
    "framework",
    "fullstack",
    "ssr",
    "web",
    "nextjs",
    "react",
    "typescript",
    "app-router"
  ],
  "author": "ovenx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oven-ttta/oven-framework.git"
  },
  "homepage": "https://oven-drab.vercel.app",
  "bugs": {
    "url": "https://github.com/oven-ttta/oven-framework/issues"
  }
}
